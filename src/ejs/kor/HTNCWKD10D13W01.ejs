                    x: () => -(container.offsetWidth - innerWidth),
                    // ease: "none",
                    scrollTrigger: {
                        id: '히스토리',
                        trigger: container,
                        start: "bottom top",
                        invalidateOnRefresh: true,
                        markers: true,
                        scrub: 1,
                        end: () => "+=" + (container.offsetWidth - innerWidth)
                    }
                })
            }
