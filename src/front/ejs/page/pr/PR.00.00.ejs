        Tab();

        //인트로 슬라이드
        var sync1 = new Swiper(".sync1", {
            slidesPerView: 1,
            loopAdditionalSlides: 1,
            effect: "fade",
            loop: true,
            controller: {
                control: sync2
            }
        });
        var sync2 = new Swiper(".sync2", {
            slidesPerView: 1,
            loopAdditionalSlides: 1,
            loop: true,
            navigation: {
                nextEl: ".sync2 .swiper-button-next",
                prevEl: ".sync2 .swiper-button-prev",
            },
            controller: {
                control: sync1
            }
        });

        sync1.controller.control = sync2;
        sync2.controller.control = sync1;

        //영상 라이브러리 슬라이드
