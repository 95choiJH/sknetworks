            slidesPerView: 3,
            allowTouchMove: true,
            spaceBetween: 24,
            loop: true,
            centeredSlides: false,
            navigation: {
                nextEl: ".pr-swiper-area .swiper-button-next",
                prevEl: ".pr-swiper-area .swiper-button-prev",
            },
            breakpoints: {
                280: {
                    slidesPerView: 'auto',
                    spaceBetween: 16,
                    centeredSlides: false,
                },
                1024: {
                    slidesPerView: 3,
                    spaceBetween: 24,
                    centeredSlides: true,
                },
            },
        });
    });
